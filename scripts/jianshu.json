{
  "key": "jianshu",
  "name": "简书",
  "type": "通用类",
  "url": "https://www.jianshu.com/sign_in",
  "logoUrl": "",
  "puppeteerScripts": {
    "processor": [],
    "version": "1.2.0",
    "login": [
      {
        "key": "cookieAuthPassKey",
        "val": "remember_user_token"
      }
    ],
    "configSchema": {
      "type": "object",
      "displayType": "row",
      "showDescIcon": true,
      "column": 1,
      "properties": {
        "mode": {
          "title": "文章编辑器",
          "default": "md",
          "required": true,
          "disabled": false,
          "readOnly": false,
          "hidden": false,
          "enum": [
            "md",
            "rich"
          ],
          "enumNames": [
            "Markdown",
            "富文本"
          ],
          "type": "string",
          "widget": "select"
        },
        "classify": {
          "title": "文集",
          "required": true,
          "disabled": false,
          "readOnly": false,
          "hidden": false,
          "enum": [],
          "enumNames": [],
          "type": "string",
          "widget": "select"
        },
        "debug": {
          "title": "是否打开渠道窗口",
          "description": "勾选则显示发布流程",
          "default": false,
          "required": false,
          "disabled": false,
          "readOnly": false,
          "hidden": false,
          "type": "boolean",
          "widget": "checkbox"
        }
      }
    },
    "configProcessing": [
      {
        "key": "publishUrl",
        "val": "https://www.jianshu.com/writer#/"
      },
      {
        "key": "publishConfig",
        "val": "\n    async function initFrom() {\n  const resp = eval({\n    classify: [...document.querySelectorAll('div.ant-col>ul>li')].reduce((acc, o) => {\n      return { ...acc, [o.innerText]: o.innerText };\n    }, {}),\n  });\n  const groupsEnum = Object.keys(resp.classify).map((o) => o);\n  const classify = {};\n  classify.enum = groupsEnum;\n  classify.enumNames = Object.values(resp.classify);\n  classify.default = groupsEnum[0];\n  return { classify: classify };\n}\n    initFrom()\n    "
      }
    ],
    "draft": [
      {
        "name": "跳转页面",
        "runs": [
          {
            "fn": "gotoUrl",
            "props": {
              "url": "https://www.jianshu.com/writer#/"
            }
          }
        ]
      },
      {
        "name": "选择文集",
        "runs": [
          {
            "fn": "clicks",
            "props": {
              "ids": [
                "//span[text()='{{this.body.classify}}']"
              ]
            }
          }
        ]
      },
      {
        "name": "等待",
        "runs": [
          {
            "fn": "wait",
            "props": {
              "time": 500
            }
          },
          {
            "fn": "clicks",
            "props": {
              "ids": [
                "//i[@class='fa fa-plus-circle']"
              ]
            }
          },
          {
            "fn": "wait",
            "props": {
              "time": 500
            }
          }
        ]
      },
      {
        "name": "发布内容",
        "runs": [
          {
            "fn": "pastes",
            "props": {
              "groups": [
                {
                  "key": "input[type='text'][value]",
                  "val": "{{this.body.title}}"
                },
                {
                  "key": "#arthur-editor",
                  "val": "{{this.body.content}}"
                }
              ]
            }
          }
        ]
      }
    ],
    "publish": [
      {
        "name": "等待",
        "runs": [
          {
            "fn": "wait",
            "props": {
              "time": 2000
            }
          }
        ]
      },
      {
        "name": "查看是否已保存",
        "runs": [
          {
            "fn": "has",
            "props": {
              "id": "//div/p[text()[normalize-space()='已保存']]",
              "type": "stop"
            },
            "yes": [
              {
                "name": "点击发布",
                "runs": [
                  {
                    "fn": "clicks",
                    "props": {
                      "ids": [
                        "//a[contains(@data-action,'publicize')]"
                      ]
                    }
                  }
                ]
              }
            ],
            "no": []
          }
        ]
      },
      {
        "name": "等待保存",
        "runs": [
          {
            "fn": "has",
            "props": {
              "id": "//a[contains(@data-action,'privatize')]",
              "type": "stop",
              "timeout": 10000
            },
            "yes": [],
            "no": []
          }
        ]
      },
      {
        "name": "保存发布地址",
        "runs": [
          {
            "fn": "executeJavaScript",
            "props": {
              "js": "\n            async function getPublishUrl() {\n  let url = document.evaluate(\n    `//a[contains(@class, '_2ajaT')]`,\n    document,\n    null,\n    XPathResult.FIRST_ORDERED_NODE_TYPE,\n    null\n  ).singleNodeValue.href;\n\n  return {\n    publish: {\n      url,\n    },\n  };\n}\n            getPublishUrl()\n          "
            }
          }
        ]
      }
    ],
    "draftRich": [],
    "publishRich": [],
    "cleanDraft": [
      {
        "name": "跳转草稿地址",
        "runs": [
          {
            "fn": "gotoUrl",
            "props": {
              "url": "https://www.jianshu.com/writer#/"
            }
          }
        ]
      },
      {
        "name": "查看是否有对应的专辑",
        "runs": [
          {
            "fn": "has",
            "props": {
              "id": "li[title='{{this.body.classify}}']",
              "type": "stop"
            },
            "yes": [
              {
                "name": "切换专辑",
                "runs": [
                  {
                    "fn": "clicks",
                    "props": {
                      "ids": [
                        "li[title='{{this.body.classify}}']"
                      ]
                    }
                  }
                ]
              }
            ],
            "no": []
          }
        ]
      },
      {
        "name": "查看是否有更多草稿",
        "runs": [
          {
            "fn": "has",
            "props": {
              "id": ".poOXI >i.fa-gear",
              "type": "stop"
            },
            "yes": [
              {
                "name": "删除草稿",
                "runs": [
                  {
                    "fn": "clicks",
                    "props": {
                      "ids": [
                        ".poOXI >i.fa-gear",
                        "//ul/li/span[text()[normalize-space()='删除文章']]",
                        "//button/span[text()[normalize-space()='确 认']]"
                      ]
                    }
                  }
                ]
              }
            ],
            "no": []
          }
        ]
      },
      {
        "name": "继续循环删除",
        "runs": [
          {
            "fn": "cleanDraft"
          }
        ]
      }
    ],
    "openDraft": [
      {
        "name": "跳转草稿地址",
        "runs": [
          {
            "fn": "gotoUrl",
            "props": {
              "url": "https://www.jianshu.com/writer#/"
            }
          }
        ]
      }
    ]
  }
}
