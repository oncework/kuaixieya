{
  "key": "csdn",
  "name": "CSDN",
  "type": "开发者",
  "url": "https://blog.csdn.net/",
  "logoUrl": "",
  "puppeteerScripts": {
    "processor": [],
    "version": "1.2.0",
    "login": [
      {
        "key": "cookieAuthPassKey",
        "val": "UserToken"
      }
    ],
    "configSchema": {
      "type": "object",
      "displayType": "row",
      "showDescIcon": true,
      "column": 1,
      "properties": {
        "mode": {
          "title": "文章编辑器",
          "default": "md",
          "required": true,
          "disabled": false,
          "readOnly": false,
          "hidden": false,
          "enum": [
            "md",
            "rich"
          ],
          "enumNames": [
            "Markdown",
            "富文本"
          ],
          "type": "string",
          "widget": "select"
        },
        "type": {
          "title": "文章类型",
          "default": "原创",
          "required": true,
          "disabled": false,
          "readOnly": false,
          "hidden": false,
          "enum": [
            "原创",
            "转载",
            "翻译"
          ],
          "enumNames": [
            "原创",
            "转载",
            "翻译"
          ],
          "type": "string",
          "widget": "radio"
        },
        "origin_url": {
          "title": "原文链接",
          "required": true,
          "disabled": false,
          "readOnly": false,
          "hidden": "{{formData.type === '原创' || formData.type === '请选择'}}",
          "props": {
            "allowClear": false
          },
          "pattern": "",
          "type": "string"
        },
        "debug": {
          "title": "是否打开渠道窗口",
          "description": "勾选则显示发布流程",
          "default": false,
          "required": false,
          "disabled": false,
          "readOnly": false,
          "hidden": false,
          "type": "boolean",
          "widget": "checkbox"
        }
      }
    },
    "configProcessing": [
      {
        "key": "publishUrl",
        "val": "https://editor.csdn.net/md/?not_checkout=1"
      },
      {
        "key": "publishConfig",
        "val": "\n      async function initFrom() {\n  return {};\n}\n      initFrom()\n    "
      }
    ],
    "draft": [
      {
        "name": "跳转页面",
        "runs": [
          {
            "fn": "gotoUrl",
            "props": {
              "url": "https://editor.csdn.net/md/?not_checkout=1"
            }
          }
        ]
      },
      {
        "name": "发布内容",
        "runs": [
          {
            "fn": "pastes",
            "props": {
              "groups": [
                {
                  "key": "//input[contains(@class,'article-bar__title article-bar__title--input')]",
                  "val": "{{this.body.title}}"
                },
                {
                  "key": ".editor__inner",
                  "val": "{{this.body.content}}"
                }
              ]
            }
          }
        ]
      },
      {
        "name": "等待初始化",
        "runs": [
          {
            "fn": "wait",
            "props": {
              "time": 1000
            }
          }
        ]
      },
      {
        "name": "打开发布界面",
        "runs": [
          {
            "fn": "clicks",
            "props": {
              "ids": [
                "//button[text()[normalize-space()='发布文章']]"
              ]
            }
          }
        ]
      },
      {
        "name": "选择文章类型",
        "runs": [
          {
            "fn": "select",
            "props": {
              "id": "select.textfield",
              "value": "{{this.body.type}}"
            }
          }
        ]
      },
      {
        "name": "勾选授权",
        "when": "'{{this.body.type}}' !== '原创'",
        "runs": [
          {
            "fn": "clicks",
            "props": {
              "ids": [
                "//label[contains(@for, 'chkAuthorized')]"
              ]
            }
          }
        ]
      },
      {
        "name": "请填写原文链接",
        "when": "'{{this.body.type}}' !== '原创'",
        "runs": [
          {
            "fn": "pastes",
            "props": {
              "groups": [
                {
                  "key": "input[placeholder=\"请填写原文链接\"]",
                  "val": "{{this.body.origin_url}}"
                }
              ]
            }
          }
        ]
      },
      {
        "name": "保存草稿",
        "only": true,
        "runs": [
          {
            "fn": "clicks",
            "props": {
              "ids": [
                "//button[text()[normalize-space()='保存为草稿']]"
              ]
            }
          }
        ]
      }
    ],
    "publish": [
      {
        "name": "点击发布按钮",
        "runs": [
          {
            "fn": "clicks",
            "props": {
              "ids": [
                "//div[contains(@class, 'modal__button-bar')]/button[text()[normalize-space()='发布文章']]"
              ]
            }
          }
        ]
      },
      {
        "name": "等待发布完成",
        "runs": [
          {
            "fn": "waitVisible",
            "props": {
              "id": "//a[contains(@class, 'toarticle c-blue')]",
              "timeout": 10
            }
          }
        ]
      },
      {
        "name": "保存发布地址",
        "runs": [
          {
            "fn": "executeJavaScript",
            "props": {
              "js": "\n            async function getPublishUrl() {\n  let url = document.evaluate(\n    `//a[contains(@class, 'toarticle c-blue')]`,\n    document,\n    null,\n    XPathResult.FIRST_ORDERED_NODE_TYPE,\n    null\n  ).singleNodeValue.href;\n\n  return {\n    publish: {\n      url,\n    },\n  };\n}\n            getPublishUrl()\n          "
            }
          }
        ]
      }
    ],
    "draftRich": [],
    "publishRich": [],
    "cleanDraft": [
      {
        "name": "跳转草稿页",
        "runs": [
          {
            "fn": "gotoUrl",
            "props": {
              "url": "https://mp.csdn.net/mp_blog/manage/article"
            }
          }
        ]
      },
      {
        "name": "切换草稿",
        "runs": [
          {
            "fn": "clicks",
            "props": {
              "ids": [
                "//ul/li/span[contains(text(),'草稿箱')]"
              ]
            }
          }
        ]
      },
      {
        "name": "查看是否有更多草稿",
        "runs": [
          {
            "fn": "has",
            "props": {
              "id": ".item-info-oper>.el-dropdown",
              "type": "stop"
            },
            "yes": [
              {
                "name": "删除草稿",
                "runs": [
                  {
                    "fn": "hover",
                    "props": {
                      "id": ".item-info-oper>.el-dropdown"
                    }
                  },
                  {
                    "fn": "wait",
                    "props": {
                      "time": 500
                    }
                  },
                  {
                    "fn": "clicks",
                    "props": {
                      "ids": [
                        "//ul[@x-placement='bottom-end']/li/div/a/span[contains(text(),'删除')]",
                        "//div/button/span[contains(text(),'确定')]"
                      ]
                    }
                  }
                ]
              }
            ],
            "no": []
          }
        ]
      },
      {
        "name": "继续循环删除",
        "runs": [
          {
            "fn": "cleanDraft"
          }
        ]
      }
    ],
    "openDraft": [
      {
        "name": "跳转草稿地址",
        "runs": [
          {
            "fn": "gotoUrl",
            "props": {
              "url": "https://mp.csdn.net/mp_blog/manage/article"
            }
          }
        ]
      }
    ]
  }
}
